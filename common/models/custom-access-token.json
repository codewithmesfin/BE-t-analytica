{
  "name": "CustomAccessToken",
  "base": "AccessToken",
  "idInjection": false,
  "options": {
    "validateUpsert": false
  },
  "properties": {
    "userInfo": {
      "type": "object"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "UserAccount",
      "foreignKey": "userId"
    },
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "userId"
    }
  },
  "acls": [{
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "*"
    }
  ],
  "methods": {}
}